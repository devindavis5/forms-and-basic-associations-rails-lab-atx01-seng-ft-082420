<%= form_for @song do |f| %>
  <%= f.label "Title:" %>
  <%= f.text_field :title %><br/>

  <%= f.label "Artist:" %>
  <%= f.text_field :artist_name %><br/>

  <%= f.label "Genre:" %>
  <%= f.collection_select :genre_id, @genres, :id, :name %><br/>

  <%= f.label "Song Notes:" %>
  <input name="song[s_notes][]" id="song_notes_1">
  <input name="song[s_notes][]" id="song_notes_2"><br></br>

  <%=f.submit%>

<%end%>